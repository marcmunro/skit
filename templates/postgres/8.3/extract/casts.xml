<?xml version="1.0" encoding="UTF-8"?>

<skit:inclusion
   xmlns:skit="http://www.bloodnok.com/xml/skit"
   xmlns:xi="http://www.w3.org/2003/XInclude">

  <skit:runsql var="casts" file="sql/casts.sql">
    <cast>
      <!-- Old code
      <skituls:expr>
	fn_oid = tuple['fn_oid']
	if fn_oid and (fn_oid != '0')
	  tuple['fn_sig'] = Globals['functions'][fn_oid]['signature']
        end
      </skituls:expr>
	-->
      <skit:attr name='name'/>
      <skit:attr name='source_type_schema'/>
      <skit:attr name='source_type'/>
      <skit:attr name='target_type_schema'/>
      <skit:attr name='target_type'/>
      <skit:attr name='context'/>
      <skit:attr name='fn_sig'/>
    </cast>
  </skit:runsql>
</skit:inclusion>

